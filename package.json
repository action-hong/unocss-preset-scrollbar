{
  "name": "unocss-preset-scrollbar",
  "version": "0.0.1",
  "packageManager": "pnpm@6.32.3",
  "description": "unocss preset for scrollbar",
  "keywords": [
    "unocss",
    "unocss-preset",
    "scrollbar",
    "unocss-preset-scrollbar"
  ],
  "homepage": "https://github.com/action-hong/unocss-preset-scrollbar#readme",
  "bugs": {
    "url": "https://github.com/action-hong/unocss-preset-scrollbar/issues"
  },
  "license": "MIT",
  "author": "kkopite <kkopitehong@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/action-hong/unocss-preset-scrollbar.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "unocss": "^0.30.6"
  },
  "dependencies": {
    "@unocss/preset-mini": "^0.31.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.6",
    "@antfu/ni": "^0.14.0",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.25",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "esno": "^0.14.1",
    "pnpm": "^6.32.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.4",
    "unocss": "^0.31.12",
    "vite": "^2.9.5",
    "vitest": "^0.9.4"
  }
}
